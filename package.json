{
  "name": "vue-dialog",
  "version": "0.1.2",
  "description": "a collection of modal dialog utilities for vue",
  "main": "dist/vue-dialog.js",
  "author": "vin maimone",
  "license": "ISC",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --host 0.0.0.0 --port 8081 --config webpack.base.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.lib.config.js",
    "docs:copy": "ncp dist docs/dist",
    "example-server": "npm run build && npm run docs:copy && cd docs && http-server -p 4002"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.26.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "ncp": "^2.0.0",
    "url-loader": "^0.5.7",
    "vue": "^2.1.7",
    "vue-hot-reload-api": "^2.0.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^10.0.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.16.0"
  }
}
